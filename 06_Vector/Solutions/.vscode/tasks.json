{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "Build main with debugging",
            "command": "/usr/bin/gcc-9",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${file}",
				"VectorTests.c",
				"Vector_int.c",
                "-o",
                "main.out"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        }
    ],
    "version": "2.0.0"
}